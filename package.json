{
  "name": "deploy",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^3.10.4",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "classnames": "^2.2.6",
    "connect-redis": "^3.4.0",
    "cross-env": "^5.2.0",
    "d3": "^4.13.0",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "formidable": "^1.2.1",
    "http-proxy-middleware": "^0.19.0",
    "ioredis": "^4.2.0",
    "lodash": "^4.17.11",
    "mobx": "^4.6.0",
    "mobx-react": "^5.3.6",
    "mobx-react-router": "^4.0.5",
    "module-alias": "^2.1.0",
    "nodemailer": "^5.1.1",
    "papaparse": "^4.6.1",
    "pm2": "^2.10.4",
    "react": "^16.6.1",
    "react-dom": "^16.6.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^1.1.5",
    "react-virtualized": "^9.21.0",
    "uuid": "^3.3.2",
    "ws": "^6.1.0"
  },
  "proxy": {
    "/ws": {
      "target": "ws://localhost:9080",
      "ws": true
    },
    "/": {
      "target": "http://localhost:9080"
    }
  },
  "scripts": {
    "start": "react-app-rewired build && rm -rf static && mv build static && node ./server/index.js",
    "dev": "react-app-rewired start & pm2 start deploy2-dev.json",
    "fdev": "react-app-rewired start & node ./server/index.js",
    "frontend": "react-app-rewired start",
    "server": "node server",
    "nodemon": "nodemon server",
    "build": "react-app-rewired build && rm -rf static && mv build static",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "babel-plugin-import": "^1.11.0",
    "mobx-react-devtools": "^5.0.1",
    "node-sass": "^4.10.0",
    "react-app-rewire-css-modules": "github:codebandits/react-app-rewire-css-modules",
    "react-app-rewire-mobx": "^1.0.9",
    "react-app-rewired": "^1.6.2",
    "react-waypoint": "^8.1.0",
    "sass-loader": "^6.0.7"
  },
  "_moduleAliases": {
    "wss": "server/webSocket",
    "logger": "server/logger",
    "redis": "server/redis",
    "config": "config",
    "restriction": "server/restriction",
    "command": "command"
  }
}
